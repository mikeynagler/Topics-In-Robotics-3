activate robot through ssh:
    ssh komodo@192.168.0.100
    export ROS_IP="192.168.0.100"
    "roslaunch robotican_komodo komodo_bringup.launch

run script:
    export ROS_MASTER_URI="http://192.168.0.100:11311"
    export ROS_IP="192.168.0.101"
    export PS1="[ROS-ROBOT]${PS1}"
    cd catkin_ws/src/assign2 $SCRIPT 
    
